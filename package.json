{
    "name": "nextjs-fullstackinator",
    "version": "1.0.0",
    "description": "Full of stack to the max. Nextjs with custom express server, Typescript, Redux, Saga, Styled-components and more",
    "private": true,
    "scripts": {
        "dev": "nodemon",
        "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
        "build:client": "next build ./client",
        "build:server": "tsc --project ./server/tsconfig.server.json",
        "start": "cross-env NODE_ENV=production node distServer"
    },
    "author": "Kyle Gallagher",
    "license": "MIT",
    "dependencies": {
        "es6-promise": "^4.2.8",
        "express": "^4.17.1",
        "next": "^9.2.2",
        "next-redux-saga": "^3.0.0",
        "next-redux-wrapper": "^5.0.0",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-actions": "^2.6.5",
        "redux-saga": "^1.1.3",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.3",
        "@types/node": "^13.9.0",
        "@types/react": "^16.9.23",
        "@types/react-dom": "^16.9.5",
        "babel-plugin-styled-components": "^1.10.7",
        "concurrently": "^5.1.0",
        "cross-env": "^7.0.2",
        "nodemon": "^2.0.2",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.3"
    }
}
